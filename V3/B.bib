@Article{Festuccia2011,
author="Festuccia, Guido
and Seiberg, Nathan",
title="Rigid supersymmetric theories in curved superspace",
journal="Journal of High Energy Physics",
year="2011",
month="Jun",
day="24",
volume="2011",
number="6",
pages="114",
abstract="We present a uniform treatment of rigid supersymmetric field theories in a curved spacetime {\$} {\backslash}mathcal{\{}M{\}} {\$}, focusing on four-dimensional theories with four supercharges. Our discussion is significantly simpler than earlier treatments, because we use classical background values of the auxiliary fields in the supergravity multiplet. We demonstrate our procedure using several examples. For {\$} {\backslash}mathcal{\{}M{\}} = Ad{\{}S{\_}4{\}} {\$}we reproduce the known results in the literature. A supersymmetric Lagrangian for {\$} {\backslash}mathcal{\{}M{\}} = {\{}{\backslash}mathbb{\{}S{\}}^4{\}} {\$}exists, but unless the field theory is conformal, it is not reflection positive. We derive the Lagrangian for {\$} {\backslash}mathcal{\{}M{\}} = {\{}{\backslash}mathbb{\{}S{\}}^3{\}} {\backslash}times {\backslash}mathbb{\{}R{\}} {\$}and note that the time direction {\$} {\backslash}mathbb{\{}R{\}} {\$}can be rotated to Euclidean signature and be compactified to {\$} {\{}{\backslash}mathbb{\{}S{\}}^1{\}} {\$}only when the theory has a continuous R-symmetry. The partition function on {\$} {\backslash}mathcal{\{}M{\}} = {\{}{\backslash}mathbb{\{}S{\}}^3{\}} {\backslash}times {\{}{\backslash}mathbb{\{}S{\}}^1{\}} {\$}is independent of the parameters of the flat space theory and depends holomorphically on some complex background gauge fields. We also consider R-invariant {\$} {\backslash}mathcal{\{}N{\}} = 2 {\$}theories on {\$} {\{}{\backslash}mathbb{\{}S{\}}^3{\}} {\$}and clarify a few points about them.",
issn="1029-8479",
doi="10.1007/JHEP06(2011)114",
url="https://doi.org/10.1007/JHEP06(2011)114"
}

